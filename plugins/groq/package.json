{
  "name": "genkitx-groq",
  "description": "Firebase Genkit AI framework plugin for Groq APIs.",
  "keywords": [
    "genkit",
    "genkit-plugin",
    "genkit-model",
    "groq",
    "llama3-8b",
    "llama3-70b",
    "mixtral-8x7b",
    "gemma-7b",
    "ai",
    "genai",
    "generative-ai"
  ],
  "version": "0.23.0",
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheFireCo/genkit-plugins.git",
    "directory": "plugins/groq"
  },
  "author": "TheFireCo",
  "license": "Apache-2.0",
  "dependencies": {
    "groq-sdk": "^0.19.0"
  },
  "peerDependencies": {
    "genkit": "^0.9.0 || ^1.0.0"
  },
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.11.16",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsup": "^8.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3"
  },
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.ts",
      "default": "./lib/index.js"
    }
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "scripts": {
    "check": "tsc",
    "compile": "tsup-node",
    "build:clean": "rimraf ./lib",
    "build": "npm-run-all build:clean check compile",
    "build:watch": "tsup-node --watch",
    "test": "tsx \"./tests/\""
  }
}
